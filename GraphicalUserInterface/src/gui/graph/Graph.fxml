<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="gui.graph.GraphController">
    <BarChart fx:id="barChart" layoutX="50" layoutY="50" prefHeight="400" prefWidth="500">
        <xAxis>
            <CategoryAxis fx:id="xAxis" label="Categories" />
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="yAxis" label="Values" />
        </yAxis>
    </BarChart>
</AnchorPane>