<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>

<TitledPane fx:id="sheetNameTitledPane" focusTraversable="false" styleClass="top-titled-pane" stylesheets="@style/TopSubComponent.css" text="Sheet Name and Version" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.gui.editor.top.TopSubComponentController">
   <content>
      <ScrollPane fitToHeight="true" fitToWidth="true" vbarPolicy="NEVER">
         <content>
            <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" styleClass="hbox">
               <children>
                  <fx:include fx:id="actionLine" source="../action/line/ActionLine.fxml" HBox.hgrow="ALWAYS" />
                  <ChoiceBox fx:id="versionsChoiceBox" onMouseClicked="#onVersionMenuClicked" prefWidth="140.0" />
               </children>
               <padding>
                  <Insets left="10.0" right="20.0" />
               </padding>
            </HBox>
         </content>
      </ScrollPane>
   </content>
   <padding>
      <Insets bottom="3.0" left="5.0" right="5.0" top="3.0" />
   </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</TitledPane>
